{
  "name": "@traqula/algebra-sparql-1-2",
  "type": "module",
  "version": "0.0.22",
  "description": "Convert SPARQL to SPARQL algebra",
  "author": "Jitse De Smet",
  "lsd:module": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/comunica/traqula.git",
    "directory": "engines/algebra-sparql-1-2"
  },
  "bugs": {
    "url": "https://github.com/comunica/traqula/issues"
  },
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/esm/lib/index.d.ts",
      "import": "./dist/esm/lib/index.js",
      "require": "./dist/cjs/lib/index.js"
    }
  },
  "main": "dist/esm/lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*.d.ts",
    "dist/**/*.js",
    "dist/**/*.js.map"
  ],
  "scripts": {
    "build": "yarn build:ts && yarn build:transpile",
    "build:ts": "node \"../../node_modules/typescript/bin/tsc\" -b",
    "build:transpile": "esbuild \"dist/esm/**/*.js\" --outdir=dist/cjs --platform=node --format=cjs"
  },
  "dependencies": {
    "@traqula/algebra-sparql-1-1": "^0.0.22",
    "@traqula/algebra-transformations-1-1": "^0.0.22",
    "@traqula/algebra-transformations-1-2": "^0.0.22",
    "@traqula/core": "^0.0.22"
  },
  "devDependencies": {
    "@rdfjs/types": "^2.0.1",
    "@traqula/generator-sparql-1-2": "^0.0.22",
    "@traqula/parser-sparql-1-2": "^0.0.22",
    "@traqula/rules-sparql-1-2": "^0.0.22",
    "@traqula/test-utils": "^0.0.22",
    "sparqlalgebrajs": "^5.0.1"
  }
}
