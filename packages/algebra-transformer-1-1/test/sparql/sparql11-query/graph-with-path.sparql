prefix : <http://example/>

select * where {
    graph ?g {
        :a :b ?x.
        :a (:p/:p)? ?t.
        ?x :c ?t.
    }
}



SELECT ?g ?t ?x WHERE {
 GRAPH ?g {
  <http://example/a> <http://example/b> ?x .
   <http://example/a> ( <http://example/p> )/( <http://example/p> )? ?t .
   ?x <http://example/c> ?t .
   }
}