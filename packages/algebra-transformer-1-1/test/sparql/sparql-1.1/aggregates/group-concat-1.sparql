PREFIX : <http://www.example.org/>
ASK {
    {SELECT (GROUP_CONCAT(?o) AS ?g) WHERE {
          [] :p1 ?o
    }}
    FILTER(?g = "1 22" || ?g = "22 1")
}



ASK WHERE {
  SELECT ( group_concat( ?o ;SEPARATOR=" " ) AS ?g ) WHERE { _:g_0 <http://www.example.org/p1> ?o . }
  FILTER ( ( ( ?g = "1 22"^^<http://www.w3.org/2001/XMLSchema#string> ) || ( ?g = "22 1"^^<http://www.w3.org/2001/XMLSchema#string> ) ) ) }
