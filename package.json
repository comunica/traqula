{
  "name": "traqula",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "yarn@4.6.0",
  "workspaces": [
    "engines/*",
    "packages/*"
  ],
  "description": "",
  "scripts": {
    "build": "yarn build:ts && yarn build:transpile",
    "build:ts": "tsc",
    "build:transpile": "yarn workspaces foreach --all -pt run build:transpile",
    "publish": "yarn workspaces foreach --all -pt npm publish",
    "clean": "git clean -dfx",
    "lint": "eslint . --cache",
    "lint-fix": "eslint . --fix",
    "test": "yarn build && vitest test --run",
    "bench": "yarn build && vitest bench --run",
    "spec:all": "yarn workspaces foreach --all -pt --include \"engines/*\" run spec:all"
  },
  "dependencies": {
    "chevrotain": "^11.0.3",
    "rdf-data-factory": "^2.0.1"
  },
  "devDependencies": {
    "@rdfjs/types": "^2.0.0",
    "@types/sparqljs": "^3.1.12",
    "@vitest/coverage-v8": "2.1.8",
    "esbuild": "^0.24.0",
    "eslint": "^8.57.0",
    "rdf-test-suite": "^2.0.0",
    "sparqljs": "^3.7.3",
    "typescript": "^5.3.3",
    "vitest": "^2.1.8"
  }
}
