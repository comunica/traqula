  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "typings": "lib/index",
  "scripts": {
    "build": "node \"../../node_modules/typescript/bin/tsc\""
  },
  "dependencies": {
    "@traqula/core": "^0.0.21",
    "@traqula/rules-sparql-1-1": "^0.0.21"
  },
  "devDependencies": {
    "@traqula/parser-sparql-1-1": "^0.0.21",
    "@traqula/test-utils": "^0.0.21"
  }
