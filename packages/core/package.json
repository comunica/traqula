{
  "name": "@traqula/core",
  "version": "0.0.0",
  "description": "Core components of Traqula",
  "lsd:module": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jitsedesmet/traqula",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/jitsedesmet/traqula/issues"
  },
  "sideEffects": false,
  "type": "module",
  "main": "lib/index.js",
  "exports": {
    "import": "./lib/index.js",
    "require": "./lib/index.cjs"
  },
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.cjs",
    "lib/**/*.js.map"
  ],
  "engines": {
    "node": ">=18.0"
  },
  "typings": "lib/index",
  "scripts": {
    "build": "npm run build:ts && npm run build:transpile",
    "build:ts": "node \"../../node_modules/typescript/bin/tsc\"",
    "build:transpile": " node \"../../node_modules/esbuild/bin/esbuild\" --format=cjs --bundle --log-level=error --outfile=lib/index.cjs lib/index.ts"
  },
  "dependencies": {
    "chevrotain": "^11.0.3",
    "rdf-data-factory": "^2.0.1"
  }
}
