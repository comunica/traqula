{
  "private": true,
  "packageManager": "npm@10.8.2",
  "name": "traqula",
  "type": "module",
  "repository": "git+https://github.com/comunica/traqula/",
  "workspaces": [
    "engines/*",
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build:ts && npm run build:transpile && npm run build:transpile-spec",
    "build:ts": "tsc",
    "build:transpile": "lerna run build:transpile",
    "build:transpile-spec": "lerna run build:transpile-spec",
    "clean": "git clean -dfx",
    "lint": "eslint . --cache",
    "lint-fix": "eslint . --fix",
    "test": "npm run build && vitest test --run",
    "bench": "npm run build && vitest bench --run",
    "spec:all": "lerna run spec:all",
    "spec:earl": "lerna run spec:earl"
  },
  "devDependencies": {
    "@rdfjs/types": "^2.0.0",
    "@types/sparqljs": "^3.1.12",
    "@vitest/coverage-v8": "2.1.8",
    "esbuild": "^0.24.0",
    "eslint": "^8.57.0",
    "lerna": "^8.1.9",
    "manual-git-changelog": "^1.0.2",
    "rdf-test-suite": "^2.0.0",
    "sparqljs": "^3.7.3",
    "typescript": "^5.3.3",
    "vitest": "^2.1.8"
  }
}
